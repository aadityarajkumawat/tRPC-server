generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model User {
  userId    String      @id
  email     String      @unique
  password  String
  AuthToken AuthToken[]
}

model AuthToken {
  authTokenId  String @id
  user         User   @relation(fields: [userId], references: [userId])
  userId       String @unique
  tokenVersion Int    @default(1)
}
